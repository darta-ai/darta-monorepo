{
  "expo": {
    "name": "darta",
    "slug": "darta",
    "scheme": "darta",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./src/assets/darta-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#817875"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.darta.darta",
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyCgJAXkO4WoPQXTf7ENx_tejgKbiem9chc"
      },
      "googleServicesFile": "./googleService/GoogleService-Info.plist", 
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["instagram"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/darta-icon.png",
        "backgroundColor": "#b58a6a"
      },
      "package": "com.darta.darta",
      "googleServicesFile": "./googleService/GoogleService-Info.plist"
    },
    "web": {
      "favicon": "./src/assets/darta-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d23d751f-88df-452b-a30f-5b5f30e3bc43"
      }
    },
    "plugins": [
      "expo-localization",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-screen-orientation",
      "expo-location",
      "./plugins/react-native-maps-plugin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos so you can create a profile."
    
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  },
  "plugins": [
    "expo-screen-orientation",
    "expo-calendar",
    "expo-location",
    "./plugins/react-native-maps-plugin",
    
    [
      "expo-build-properties",
      {
        "ios": {
          "useFrameworks": "static"
        }
      }
    ]
  ]
}
