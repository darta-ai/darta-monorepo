{
  "expo": {
    "name": "darta",
    "slug": "darta",
    "scheme": "darta",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/darta-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#989898"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.darta.darta",
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyCgJAXkO4WoPQXTf7ENx_tejgKbiem9chc"
      },
      "LSApplicationQueriesSchemes" : ["darta"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/darta-icon.png",
        "backgroundColor": "#b58a6a"
      }
    },
    "web": {
      "favicon": "./src/assets/darta-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d23d751f-88df-452b-a30f-5b5f30e3bc43"
      }
    },
    "plugins": [
      "expo-localization",
      "@react-native-firebase/app",
      "@react-native-firebase/perf",
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  },
  "plugins": [
    [
      "expo-screen-orientation",
      "expo-calendar",
      "expo-location",
      {
        "initialOrientation": "PORTRAIT",
        "calendarPermission": "The app needs to access your calendar.",
        "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
      }
    ]
  ]
}
