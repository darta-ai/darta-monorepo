{
  "expo": {
    "name": "darta",
    "slug": "darta",
    "scheme": "darta",
    "version": "1.0.12",
    "orientation": "portrait",
    "icon": "./src/assets/darta-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#817875"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.darta.darta",
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyCgJAXkO4WoPQXTf7ENx_tejgKbiem9chc"
      },
      "googleServicesFile": "./googleService/GoogleService-Info.plist", 
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["instagram", "instagram-stories"]
      },
      "associatedDomains": [
        "applinks:darta.art",
        "applinks:c6de-2603-7000-4001-17cf-7506-479b-a1e9-487a.ngrok-free.app"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/darta-icon.png",
        "backgroundColor": "#b58a6a"
      },
      "package": "com.darta.darta",
      "googleServicesFile": "./googleService/GoogleService-Info.plist"
    },
    "web": {
      "favicon": "./src/assets/darta-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d23d751f-88df-452b-a30f-5b5f30e3bc43"
      }
    },
    "plugins": [
      "expo-localization",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/perf",
      "expo-screen-orientation",
      "./plugins/react-native-maps-plugin",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Darta uses your location for map precision, showing your position relative to galleries. E.g., finding the nearest art exhibit."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Darta needs photo access to customize your profile. E.g.,: Updating your profile picture to your latest artwork."
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "Darta integrates with your calendar for event scheduling. E.g., setting reminders for gallery openings or visits."
        }
      ],      
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  },
  "plugins": [
    "expo-screen-orientation",
    "expo-calendar",
    "expo-location",
    "./plugins/react-native-maps-plugin",
    [
      "expo-build-properties",
      {
        "ios": {
          "useFrameworks": "static"
        }
      }
    ]
  ]
}
