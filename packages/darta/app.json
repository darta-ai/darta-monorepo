{
  "expo": {
    "name": "darta",
    "slug": "darta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/darta-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/darta-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.darta"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/darta-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/darta-icon.png"
    }
  },
  "plugins": [
    [
      "expo-screen-orientation",
      {
        "initialOrientation": "PORTRAIT"
      }
    ]
  ]
}
